{
  "email": {
    "type"     : "String",
    "required" : true,
    "trim"     : true,
    "minlength": 1,
    "unique"   : true
  },
  "password": {
    "type"     : "String",
    "required" : true,
    "minlength": 6
  },
  "fullName": {
    "type"     : "String",
    "required" : true,
    "minlength": 5
  },
  "tokens": [{
    "access": {
      "type"    : "String",
      "required": true
    },
    "token": {
      "type"    : "String",
      "required": true
    }
  }]
}